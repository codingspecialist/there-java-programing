package ch05;

abstract class 홀직원{
	abstract void 청소();
}

abstract class 종업원 extends 홀직원{
	void 서빙하기() {
		System.out.println("서빙하기");
	}
	void 주문받기() {
		System.out.println("주문받기");
	}
}

public class OOPEx10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
