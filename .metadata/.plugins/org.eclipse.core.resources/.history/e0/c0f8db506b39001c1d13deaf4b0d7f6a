package ch05;

class Engine {
	int power = 2000;
}

class Car { // 자동차는 엔진이 아니기 때문에 상속할 수 없다.
	
	// 콤포지션!! 결합
	Engine e;
	
	public Car(Engine e) {
		this.e = e;
	}
}

class Hamburger {
	String name = "기본햄버거";
}

public class OOPEx02 {

	public static void main(String[] args) {
		Engine e1 = new Engine();
		Car c1 = new Car(e1);
		System.out.println("자동차의 엔진 마력은 : "+c1.e.power);
	}

}
