package ch07;

public class ThreadEx01 {

	// 자바의 메인 스레드
	public static void main(String[] args) {
		for(int i=1; i<11; i++) {
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.println("메인스레드 : "+i);
		}
	}

}
