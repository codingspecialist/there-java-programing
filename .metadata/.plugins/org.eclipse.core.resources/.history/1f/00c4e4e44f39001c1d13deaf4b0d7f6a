package ch05;

class Player {
	String name; // 이름
	int thirty; // 목마름
	
	public Player(String name, int thirty) {
		this.name = name;
		this.thirty = thirty;
	}
}

public class OOPEx01 {

	public static void main(String[] args) {
		Player p1 = new Player("홍길동", 100);
		System.out.println(p1.name);
		System.out.println(p1.thirty);
	}

}
