package ch07;

class SubThread implements Runnable{

	// 자바의 서브 스레드
	@Override
	public void run() {
		
	}
}

public class ThreadEx01 {

	// 자바의 메인 스레드
	public static void main(String[] args) {
		for(int i=1; i<6; i++) {
			try {
				System.out.println("메인스레드 : "+i);
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

}
